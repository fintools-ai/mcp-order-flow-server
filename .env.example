# Data source configuration
# Options: 'redis' (direct Redis) or 'grpc' (high-performance gRPC - recommended)
DATA_SOURCE=grpc

# gRPC Data Broker configuration (when DATA_SOURCE=grpc) - FASTEST OPTION
DATA_BROKER_GRPC_URL=localhost:9090

# Redis configuration (when DATA_SOURCE=redis)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# Logging
LOG_LEVEL=INFO